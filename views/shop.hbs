
<div id="store" class="col-md-12 ">
						<div class="row">
    {{#each rows}} 

<!-- product -->
							<div class="col-md-4 col-xs-6">
								<div class="product">
									<div class="product-img">
										<img class="img" src="{{this.img}}" alt="">
										
									</div>
									<div class="product-body">
										<div class="product-rating">
											<i class="fa fa-star"></i>
											<i class="fa fa-star"></i>
											<i class="fa fa-star"></i>
											<i class="fa fa-star"></i>
											<i class="fa fa-star"></i>
										</div>
									 <h3 class="product-name"><a href="#">{{this.title}}</a></h3>
									 <h4 class="product-price">{{this.price}} جنيه<del class="product-old-price"></del></h4>
										
										
									</div>
									<div class="add-to-cart">
										<a href="/view/{{this.id}}"><button class="add-to-cart-btn"><i class="fa fa-eye"></i>view</button></a>
									</div>
								</div>
							</div>
                  {{/each}}

              							</div>
</div>
							<!-- /product -->
